[{
 	"name": "ping",
 	"description": "Check network connectivity in Layer 3",
 	"interval": 5,
 	"exec": "ping.sh",
 	"command": "ping.sh 8.8.8.8",
 	"type": "gauge",
    "labels":{"dest":"8.8.8.8"}
 }, {
 	"name": "active_connections",
 	"description": "nginx active connections",
 	"interval": 4,
 	"exec": "get_active_cons.sh",
 	"command": "get_active_cons.sh",
 	"type": "gauge",
    "labels":{"application":"nginx_server"}
 }]